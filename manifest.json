{
  "manifest_version": 2,

  "name": "IP ANALYZER",
  "description": "This extension will analyze IP address",
  "version": "1.0",
  "background": {
  "scripts": ["background.js"]
  },

  "browser_action": {
   "default_icon": "extension_logo.png",
   "default_popup": "popup.html"
  },
  
  "permissions": [
   "activeTab", "tabs", "background"
   ]
}